{
  "deno.enable": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "files.autoSave": "onFocusChange",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.rulers": [
    120
  ],
  "cSpell.words": [
    "BECSY",
    "behavior",
    "Bitfield",
    "Bitfields",
    "bitflags",
    "bitpool",
    "Bitpool",
    "BooleanArray",
    "bunx",
    "CODEOWNERS",
    "codepaths",
    "Deno",
    "devtool",
    "dotfiles",
    "downlevel",
    "ECSY",
    "endregion",
    "esmodules",
    "ESNEXT",
    "flatmap",
    "gamedev",
    "Geotic",
    "HECS",
    "msvc",
    "nand",
    "onner",
    "outdir",
    "Parens",
    "phughesmcr",
    "popcnt",
    "popcount",
    "postbuild",
    "sourcemap",
    "stringifier",
    "treeshake",
    "tsbuildinfo",
    "tslib",
    "typechecking",
    "xnor"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
